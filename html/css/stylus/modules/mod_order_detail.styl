orderdetail()
  .ordertitle
    width 100%
    height 1.09375rem
    line-height 1.09375rem
    background-color #fff
    color #333333
    font-size 0.4375rem
    padding 0 0.23475rem
    display block
    .backedit,.allnum,.couponedit
      color #333
      font-size 0.375rem
  .addstyle
    .editbtntext
      color #333
      font-size 0.375rem
  .contact
    width 100%
    //position relative
    p
      border 0.015625rem dashed #B2B2B2
      border-radius(0.09375rem)
      width 80%
      margin 0.9375rem auto
      color #333333
      font-size 0.375rem
      padding 0.4375rem 0
    .icon-right
      /*position absolute*/
      /*right 1.1875rem*/
      /*top 50%*/
      font-size 0.3125rem
    .redback
      background-color #FF0000
      width 0.4375rem
      height 0.4375rem
      border-radius(0.4375rem)
      color #fff
      text-align center
      vertical-align middle
  .contactinfor
    width 80%
    padding 1.09375rem 0 1.09375rem 0.4rem
    p
      text-align left
      color #333333
      font-size 0.375rem
      line-height 1.8
      border none
      border-radius 0
      width auto
      padding 0
      margin 0
  .opbtn
    display block
    background-color #F3F3F3
    padding 0 0.23475rem
    height 1.09375rem
    line-height 1.09375rem
    margin 0.375rem 0
    em
      font-size 0.4375rem
      color #333333
    .iconfont
      font-size 0.4375rem
      color #999
    .sinfor
      color #333333
      font-size 0.375rem
      em
        color #FF0000
        font-size 0.3125rem
      cbicon()
  .editwrap
    width 95%
    margin 0.25rem auto 0.625rem auto
    border-radius(0.09375rem)
    padding 0.25rem
    height 1.90625rem
    textarea
      border none
      outline none
      background-color #fff
      color #999999
      width 100%
      font-size small
      //font-size 0.375rem
  .listinfors
    margin 0 0.23475rem
    border-bottom 0.015625rem solid #C7C7C7
    padding-top 0.15625rem
    padding-bottom 0.3125rem
    span
      color #333333
      font-size 0.375rem
      margin 0.15625rem 0 0 0
      cbicon()
  .alltext
    padding 0.25rem 0.23475rem
    p
      color #FF0000
      font-size 0.375rem
      text-align right
      cbicon()
  .textwrap
    background-color #F3F3F3
    padding 0.4rem 0
  .textp
    color #333333
    font-size 0.375rem
    line-height 1.8
    padding 0.0625rem 0.4rem
    em
      color #ff0000
  .allprice
    background-color #fff
    color #333
    text-align right
    font-size 0.375rem
    display block
    padding 0.40625rem 0.23475rem
    span
      color #333
    em
      color #ff0000
    cbicon()
  .payallbtns
    margin-top 1.5625rem
    .paybtn
      display block
      width 95%
      height 1.09375rem
      line-height 1.09375rem
      text-align center
      color #fff
      font-size 0.4375rem
      margin 0.3125rem auto
      border-radius(0.09375rem)
      .iconfont
        color #fff
        display inline-block
        margin-right 0.15625rem
    .weixinbtn
      background-color #78C422
      .icon-logoweixin
        font-size 0.7734rem
    .zhifubaobtn
      background-color #00B0F5
      .icon-zhifubao
        font-size 0.625rem
    .closebtn
      background-color #ADADAD
    .confirmPayBtn
      background-color #F05D4E