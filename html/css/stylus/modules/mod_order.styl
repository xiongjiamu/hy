order()
  .orderwrap
    width 7.59375rem
    height 4.921875rem
    margin 1.984375rem auto 0 auto
    border 0.015625rem solid #CACACA
    padding 1.40625rem 0.46875rem 0 0.46875rem
    border-radius(0.156255rem)
    position relative
    z-index 10
    .blank
      display block
      position absolute
      top 0
      left 50%
      width 1.875rem
      height 0.9375rem
      margin-left -0.9375rem
      background-color #fff
      z-index 0
    &:before
      content ' '
      background-color #fff
      width 1.875rem
      height 1.875rem
      border-radius(1.875rem)
      border 0.015625rem solid #CACACA
      display block
      position absolute
      top -0.9375rem
      left 50%
      margin-left -0.9375rem
      z-index 0
    p
      color #666666
      line-height 2.2
      font-size 0.375rem
      em
        color #F05D4E
      .mail
        color #6081a3
        text-decoration underline
    .gwcp
      color #666
      font-size 0.4375rem
    .icon-eduface4,.icon-wenjianicon
      font-size 1.0625rem
      color #5D5D65
      position absolute
      left 50%
      top -0.73125rem
      margin-left -0.53125rem
      z-index 1
    .icon-right
      color #18A80F
      width 1.10625rem
      height 1.10625rem
      border-radius(1.10625rem)
      display block
      border 0.0625rem solid #18A80F
      text-align center
      vertical-align middle
      font-size 0.78125rem
      position absolute
      left 50%
      top -0.553125rem
      margin-left -0.553125rem
      z-index 1
    .icon-gouwuche
      font-size 1.0625rem
      color #5D5D65
      position absolute
      left 50%
      top -0.73125rem
      margin-left -0.53125rem
      z-index 1
  .blankinfor
    margin-top 0.9375rem
    .title
      display block
      color #333333
      font-size 0.4375rem
      border-top 0.015625rem solid #CACACA
      background-color #fff
      height 1.09375rem
      line-height 1.09375rem
      padding 0 0.23475rem
    .blankcontent
      width 8.75rem
      margin 0.46875rem auto
      border 0.015625rem dashed #A2CA61
      border-radius(0.09375rem)
      background-color #F5FFEC
      padding 0 0.46875rem
      .divlist
        border-bottom 0.015625rem solid #CACACA
        padding 0.46875rem 0
        span
          display block
          color #333333
          font-size 0.375rem
          line-height 1.8

        em
          display block
          color #333333
          font-size 0.28rem
          line-height 1.8
          font-family Arial
        &:last-child
          border none

  .payallbtns
    margin-top 1.5625rem
    .paybtn
      display block
      width 90%
      height 1.09375rem
      line-height 1.09375rem
      text-align center
      color #fff
      font-size 0.4375rem
      margin 0.3125rem auto
      border-radius(0.09375rem)
      .iconfont
        color #fff
        display inline-block
        margin-right 0.15625rem
    .confirmbtn
      background-color #F05D4E
    .weixinbtn
      background-color #78C422
      .icon-logoweixin
        font-size 0.7734rem
    .zhifubaobtn
      background-color #00B0F5
      .icon-zhifubao
        font-size 0.625rem
    .closebtn
      background-color #ADADAD
